{
    "name": "mendoframework/logwriter-twitter",
    "description": "Mendo Twitter Log Writer Component",
    "license": "BSD-3-Clause",
    "authors": [
        {
            "name": "Mathieu Decaffmeyer",
            "email": "mdecaffmeyer@gmail.com"
        }
    ],
    "keywords": [
        "mf", "mendoframework", "mendo framework",
        "logger", "logging", "log", "log writer",
        "twitter", "tweet"
    ],
    "homepage": "https://github.com/mendoframework",
    "autoload": {
        "psr-4": {
            "Mendo\\Logger\\Writer\\": "src/"
        }
    },
    "repositories": [
        {
            "type": "vcs",
            "url": "https://github.com/mdecaffmeyer/twitter-api-php"
        }
    ],
    "require": {
        "php": ">=5.5.0",
        "psr/log": "1.0.*@dev",
        "mendoframework/logger": "~1.0",
        "j7mbo/twitter-api-php": "dev-master"
    },
    "require-dev": {
        "pimple/pimple": "3.0.*@dev",
        "phpunit/phpunit": "4.3.*@dev",
        "fabpot/php-cs-fixer": "*@dev"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0.x-dev"
        }
    }
}