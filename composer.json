{
    "name": "gobline/logwriter-twitter",
    "description": "Twitter Log Writer component",
    "license": "BSD-3-Clause",
    "authors": [
        {
            "name": "Mathieu Decaffmeyer",
            "email": "mdecaffmeyer@gmail.com"
        }
    ],
    "keywords": [
        "gobline",
        "logger", "logging", "log", "log writer",
        "twitter", "tweet"
    ],
    "homepage": "https://github.com/gobline",
    "autoload": {
        "psr-4": {
            "Gobline\\Logger\\Writer\\": "src/"
        }
    },
    "repositories": [
        {
            "type": "vcs",
            "url": "https://github.com/mdecaffmeyer/twitter-api-php"
        }
    ],
    "require": {
        "php": ">=5.6.0",
        "psr/log": "1.0.*@dev",
        "gobline/logger": "~2.0",
        "j7mbo/twitter-api-php": "dev-master"
    },
    "require-dev": {
        "pimple/pimple": "3.0.*@dev",
        "phpunit/phpunit": "4.3.*@dev",
        "fabpot/php-cs-fixer": "*@dev"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "2.0.x-dev"
        }
    }
}